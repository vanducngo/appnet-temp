    <!-- Overview-->
    <string name="overview_toeic_stand">TOEIC - Test Of English for International Communication</string>
    <string name="overview_toeic_introduction">TOEIC là chứng chỉ tiếng Anh quốc tế về giao tiếp dành cho người đi làm không phải là dân sử dụng tiếng Anh làm tiếng mẹ đẻ.</string>
    <string name="overview_toeic_structure">Bài thi TOEIC là một bài thi trắc nghiệm làm trên giấy (paper and pencil, multiple-choice test) gồm có hai thành phần (tổng thời gian làm bài là 120 phút, tổng số câu hỏi là 200 câu) cụ thể như sau:</string>
    <string name="overview_listening">Listening Comprehension (100 câu hỏi, 45 phút làm bài)</string>
    <string name="overview_listening_photographs">Part 1: Photographs – 10 câu hỏi.</string>
    <string name="overview_listening_question_response">Part 2: Question and Response – 30 câu hỏi.</string>
    <string name="overview_listening_short_conversation">Part 3: Short Conversation – 30 câu hỏi hội thoại.</string>
    <string name="overview_listening_short_talk">Part 4: Short Talks – 30 câu hỏi về bài nói.</string>
    <string name="overview_reading">Phần II: Reading (100 câu hỏi, 75 phút làm bài)</string>
    <string name="overview_reading_incomplete_sentences">Part 5: Incomplete Sentences – 40 câu hỏi hoàn thành câu.</string>
    <string name="overview_reading_text_completion">Part 6: Text completion – 12 câu hỏi.</string>
    <string name="overview_reading_conprehension">Part 7: Comprehension – 48 câu hỏi.</string>
    <string name="vocabulary_popup_close_string">Đóng</string>
    <string name="overview_full"></string>

